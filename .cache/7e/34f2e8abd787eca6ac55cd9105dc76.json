nnect(context.shape, context.source, context.connection);\n    } else {\n      this._modeling.connect(context.source, context.shape, context.connection);\n    }\n  }\n};\n\n\nfunction existsConnection(source, target) {\n  return some(source.outgoing, function(c) {\n    return c.target === target;\n  });\n}"},"lineCount":null}},"error":null,"hash":"405a01d4b4c8decc3fb1f44eb52a682e","cacheData":{"env":{}}}